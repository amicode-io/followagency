{
  "name": "humblescroll-nuxt",
  "version": "1.0.10",
  "description": "Nuxt module for Humblescroll Vue",
  "repository": "mathiasoxholm/humblescroll-nuxt",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@nuxt/kit": "^3.11.2",
    "humblescroll-vue": "1.2.1"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.3.10",
    "@nuxt/module-builder": "^0.6.0",
    "@nuxt/schema": "^3.11.2",
    "@nuxt/test-utils": "^3.12.1",
    "@types/node": "^20.12.7",
    "changelogen": "^0.5.5",
    "eslint": "^9.1.1",
    "nuxt": "^3.11.2",
    "vitest": "^1.5.2"
  },
  "overrides": {
    "vitest": "^1.5.2"
  },
  "keywords": [
    "vue",
    "nuxt",
    "nuxtjs",
    "nuxt-module",
    "tailwindCSS",
    "tailwind",
    "animations",
    "AOS",
    "scroll animations"
  ],
  "scripts": {
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "pnpm test && pnpm prepack && changelogen --release && pnpm publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  }
}